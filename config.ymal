proxy-groups:
  - name: "代理"
    type: select
    proxies:
      - 最快🚗
      - 西部数据-auto
      - 西部数据-fallback
      - GLaDOS
      - 大机场
      - 手动

  - name: "最快🚗"
    type: url-test
    use:
      - westdata
      - 大机场
      - glados
    url: 'http://www.gstatic.com/generate_204'
    interval: 300

  - name: "手动"
    type: select
    use:
      - westdata
      - 大机场
      - glados

  - name: "Spotify"
    type: select
    proxies:
      - DIRECT
      - 最快🚗
      - 西部数据-auto
      - 西部数据-fallback
      - GLaDOS
      - 大机场
      - 手动
  
  - name: "流媒体"
    type: select
    proxies:
      - 代理
      - DIRECT
      - 最快🚗
      - 西部数据-auto
      - 西部数据-fallback
      - GLaDOS
      - 大机场
      - 手动

  - name: "油管"
    type: select
    proxies:
      - 代理
      - 最快🚗
      - 西部数据-auto
      - 西部数据-fallback
      - GLaDOS
      - 大机场
      - 手动

  - name: "电报"
    type: select
    proxies:
      - 代理
      - 最快🚗
      - 西部数据-auto
      - 西部数据-fallback
      - GLaDOS
      - 大机场
      - 手动

  - name: "GLaDOS"
    type: url-test
    use:
      - glados
    url: 'http://www.gstatic.com/generate_204'
    interval: 1800

  - name: "大机场"
    type: url-test
    use:
      -  大机场
    url: 'http://www.gstatic.com/generate_204'
    interval: 1800

  - name: "西部数据-auto"
    type: url-test
    use:
      -  westdata
    url: 'http://www.gstatic.com/generate_204'
    interval: 1800
    
  - name: "西部数据-fallback"
    type: fallback
    use:
      -  westdata
    url: 'http://www.gstatic.com/generate_204'
    interval: 1800

